<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.pig4cloud.pigx.admin.mapper.QpOperatorMapper">

  <resultMap id="qpOperatorMap" type="com.pig4cloud.pigx.admin.entity.QpOperator">
                  <id property="id" column="id"/>
                        <result property="userId" column="user_id"/>
                        <result property="name" column="name"/>
                        <result property="type" column="type"/>
                        <result property="createat" column="createAt"/>
                        <result property="createdate" column="createDate"/>
                        <result property="modifyat" column="modifyAt"/>
                        <result property="modifydate" column="modifyDate"/>
                        <result property="delFlag" column="del_flag"/>
                        <result property="deptId" column="dept_id"/>
                        <result property="tenantId" column="tenant_id"/>
						<result column="id_number" property="idNumber"/>
						<result column="filling_certificate_no" property="fillingCertificateNo"/>
						<result column="filling_certificate_photo" property="fillingCertificatePhoto"/>
						<result column="inspector_information" property="inspectorInformation"/>
						<result column="inspector_company" property="inspectorCompany"/>
						<result column="inspector_name" property="inspectorName"/>
						<result column="phone" property="phone"/>
            </resultMap>
</mapper>
